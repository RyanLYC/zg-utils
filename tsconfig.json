{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "suppressImplicitAnyIndexErrors": true,
    "downlevelIteration": true,
    "noImplicitThis": false,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "types": ["jest"],
    "typeRoots": ["./node_modules/@types", "./types"],
    "lib": ["es5", "es2015", "esnext", "dom", "dom.iterable", "scripthost"],
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*.ts", "tests/**/*.ts", "types/**/*.d.ts", "tests/function/getObjValues.spec ts", "tests/function/getObjValues.spec ts"],
  "exclude": ["node_modules"]
}
